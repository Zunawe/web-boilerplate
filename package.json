  "scripts": {
    "postinstall": "npx husky install",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist node_modules .husky/_",
    "start": "node dist/index.js",
    "dev": "ts-node .",
    "test": "npm run test:client && npm run test:server",
